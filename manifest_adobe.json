{
    "manifest_version": 3,
    "name": "CampAssist.ai for AJO",
    "version": "0.0.1",
    "description": "Chrome Extenstion to enable CampAssist.ai for AJO ",
    "icons": {
        "16": "images/campassist-16.png",
        "32": "images/campassist-32.png",
        "48": "images/campassist-48.png",
        "128": "images/campassist-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://journeys.adobe.com/journey/*"
            ],
            "js": [
                "scripts/camp-assist-0.1.js"
            ],
            "run_at": "document_idle"
        }
    ]
}